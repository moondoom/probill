[database]
DATABASE_USER = probill
DATABASE_PASSWORD = probill
DATABASE_HOST = localhost
DATABASE_PORT = 
DATABASE_ENGINE = django.db.backends.postgresql_psycopg2
DATABASE_NAME = probill

